  "cmake.sourceDirectory": "${workspaceFolder}/.",
  "C_Cpp.intelliSenseEngine": "Tag Parser",
  "cmake.buildDirectory": "${workspaceFolder}/build",
  "cmake.configureSettings": {
    "CMAKE_PREFIX_PATH": [
      "${env:HOME}/usr",
    ],
    "BUILD_SHARED_LIBS": "ON",
    "CMAKE_STATIC_LINKER_FLAGS": "",
    "CMAKE_SHARED_LINKER_FLAGS": "-fuse-ld=lld -L/usr/local/lib -L${env:HOME}/usr/lib",
    "CMAKE_EXE_LINKER_FLAGS": "-fuse-ld=lld -L/usr/local/lib"
  },
  "clang-format.language.cpp.fallbackStyle": "LLVM",
  "clang-format.language.c.style": "Microsoft",
  "clang-format.language.cpp.style": "Microsoft",
  "languageToolLinter.languageTool.ignoredWordsInWorkspace": [
    "covesa"
  ],
